{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "strictNullChecks": true,
        "baseUrl": ".",
        "paths": {
            "@c/*": ["src/components/*"],
            "@b/*": ["src/components/blocks/*"],
            "@e/*": ["src/components/elements/*"],
            "@md/*": ["src/components/md/*"],
            "@l/*": ["src/layouts/*"],
            "@p/*": ["src/pages/*"],
            "@u/*": ["src/utils/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}
